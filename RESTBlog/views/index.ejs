<% include partials/header %>

<h1>Index Page!!!</h1>

<% blogs.forEach((blog) => { %>
  <div>
    <h2><%= blog.title %></h2>
    <img src="<%=blog.image%>">
    <span><%=blog.created%></span>
    <p class="index-blog-body"><%=blog.body%></p>
  </div>
  <div><a href="/blogs/<%=blog._id%>">More Info</a></div>
<% }); %>

<% include partials/footer %>